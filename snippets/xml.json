    "{N} Password Field": {
        "prefix": "nspassword",
        "body": [
            "<TextField hint=\"Enter your password.\" text=\"{{ $1 }}\" secure=\"true\" />",
            "$2"
        ]
    },
    "{N} Editable TextView": {
        "prefix": "nstextview_editable",
